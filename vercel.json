{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.php",
        "use": "@vercel/php"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/pages/$1"
      }
    ],
    "functions": {
      "api/**/*.php": {
        "runtime": "vercel-php@latest"
      }
    }
  }
  